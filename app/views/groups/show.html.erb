<h3><%= @group.title %></h3>
Creator: <%= link_to @group.user.full_name, user_path(@group.user) %><br/>
Created: <%= @group.created_at.to_s %>
<hr/>
<table class="table table-striped table-bordered table-hover">
  <thead><tr><th>Members</th></tr></thead>
  <tbody>
    <% @group.users.each do |user| %>
    <tr>
      <td><%= link_to user.full_name, user_path(user) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
